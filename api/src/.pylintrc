# Pylint configuration file
# Author: Andrew Jarombek
# Date: 1/14/2023

[MAIN]

fail-under=8.17
py-version=3.8

[MESSAGES CONTROL]

disable=
    cyclic-import,
    duplicate-code,
    invalid-name,
    logging-fstring-interpolation,
    missing-class-docstring,
    too-many-lines,
    too-many-public-methods,
    too-many-statements

[REPORTS]

output-format=text

[BASIC]

good-names=i,j,k

[EXCEPTIONS]

max-line-length=200